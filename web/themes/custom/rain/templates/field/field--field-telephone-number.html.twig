{% embed 'field.html.twig' with {
  'margin': 0,
  'classes': [
    'mx-1',
    'flex',
  ],
  'item_classes': [
    'p-2',
    'bg-cyan',
    'text-white',
    'border-2',
    'border-cyan-darker',
    'rounded',
  ],
} %}
  {% block label %}{% endblock %}
  {% block content %}
    {% set link_classes = link_classes|default([
  'flex',
  'items-center',
]) %}
    {%- for item in items -%}
      <div{{item.attributes.addClass(item_classes)}}>
        <a class="{{ link_classes|join(' ') }}" href="{{ item.content['#url'].toString() }}">
          {% include active_theme_path() ~ '/icons/phone.svg' %}
        </a>
      </div>
    {%- endfor -%}
  {% endblock %}
{% endembed %}
